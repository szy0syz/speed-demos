#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhenyu.shi/development/speed-demos/highking/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zhenyu.shi/development/speed-demos/highking/node_modules/.pnpm/node_modules"
fi
"$basedir/../../../../../esbuild@0.14.43/node_modules/esbuild/bin/esbuild"   "$@"
exit $?
